<%- include('../layout', { title: 'Users' }) %>
<h3>Users</h3>
<table class="table">
  <thead><tr><th>ID</th><th>Username</th><th>Admin</th></tr></thead>
  <tbody>
    <% users.forEach(u => { %>
      <tr><td><%= u.id %></td><td><%= u.username %></td><td><%= u.is_admin ? 'Yes' : 'No' %></td></tr>
    <% }) %>
  </tbody>
</table>
